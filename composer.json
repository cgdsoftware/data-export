{
    "name": "cgdsoftware/data-export",
    "description": "Structure for holding Laravel-Enso exports",
    "keywords": [
        "data-export",
        "cgdsoftware",
        "enso-exports"
    ],
    "homepage": "https://github.com/cgdsoftware/data-export",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://cgdsoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "box/spout": "^3.0",
        "cgdsoftware/core": "^8.0",
        "cgdsoftware/enums": "^2.0",
        "cgdsoftware/helpers": "^2.0",
        "cgdsoftware/io": "^2.0",
        "cgdsoftware/files": "^5.0",
        "cgdsoftware/track-who": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\DataExport\\": "src/",
            "LaravelEnso\\DataExport\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\DataExport\\AppServiceProvider",
                "LaravelEnso\\DataExport\\AuthServiceProvider",
                "LaravelEnso\\DataExport\\EnumServiceProvider"
            ]
        }
    }
}